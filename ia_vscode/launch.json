    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/root/oneVPL/_build/sample_decode",
            "args": ["-vaapi", "-hw", "-i", "/media/H265_Videos/Demuxed_H265_Videos/Coastguard_640x320_1mbps_30fps_Main_at_L2.1.h265", "-o", "/dev/null"],
            "cwd": "/home/root/oneVPL/_build",

            "stopAtEntry": true,

            "environment": [
                {
                    "name": "PATH",
                    "value": "/home/root/oneVPL/_build:$PATH"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "logging": {
                "engineLogging": true
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}